
  <section class="main-container">
  <div class="container">

    <section class="state-index">
      <% @states.each do |s| %>
        <div class="state-container">
          <div class="state"><%= image_tag(image_route(s)) %></div>
          <hr>
          <h1><%= s[0] %></h1>
          <% s[1].each do |l| %>
            <h2><%= link_to l, location_path(l.parameterize) %></h2>
          <% end %>
        </div>
    <% end %>
    </section>
  </div>
  </section>
