
  <section class="main-container">
  <div class="container">
    <h1>Chapter Index</h1>


	<h1>Locations</h1>
	<% @location.each do |l| %>
    </br><%= link_to l.location, location_path(l.id) %>
    </br><%= l.blurb %>
    </br><%= l.fb %>
    </br><%= l.meetup %>
    </br><%= l.geo %>
    </br><%= l.latitude %>
    </br><%= l.longitude %>
  <% end %>

    <div class="state-index">
      <% @location.each do |l| %>
        <div class="state-container">
          <div class="state">
            <%= image_tag("NC.png") %>
            <!-- <%= image_tag("NC.png") %> -->
          </div>
          <hr>
          <h1>State Name</h1>
          <h2><%= link_to l.location, location_path(l.id) %></h2>
        </div>
      <% end %>
    </div>

  </div>
  </section>
