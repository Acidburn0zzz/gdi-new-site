<h1>The Sponsors</h1>
  <% @sponsors.each do |sponsor| %>
    <br><%= sponsor.name %>
    <br><%= sponsor.url %>
    <br><%= sponsor.image.thumb.url %>
    <br><%= sponsor.location.id %>
    <br>
    <% if sponsor.image? %>
      <%= link_to image_tag(sponsor.image.thumb.url), sponsor.url %>
    <% end %>
  <% end %>
    </aside>

    <section ="main">
      <div class="classes">
        <h2>Upcoming Classes</h2>
<!--         some code to make the meetup API data show up!
 -->
      </div>


      <h2>Chapter Leaders</h2>
        <div class="leaders">
          <% @bios.each do |bio| %>
            <ul>
              <li><%= bio.title %></li>
              <li><%= bio.name %></li>
              <li><%= bio.info %></li>
              <li><%= bio.admin_user.id %></li>
              <li>
            </ul>
            <% if bio.image? %>
              <%= image_tag bio.image.thumb.url %>
            <% end %>
          <% end %>
      </div>

      <h2>The Instructors</h2>
        <div class="instructors">
          <% @bios.each do |bio| %>
            <ul>
              <li><%= bio.title %></li>
              <li><%= bio.name %></li>
              <li><%= bio.info %></li>
              <li><%= bio.admin_user.id %></li>
              <li>
            </ul>
            <% if bio.image? %>
              <%= image_tag bio.image.thumb.url %>
          <% end %>
        <% end %>
      </div>
    </section>
