<h1>Welcome to <%= @location.location %></h1>
  <ul>
    <br><%= @location.blurb %>
    <br><%= @location.fb %>
    <br><%= @location.meetup %>
    <br><%= @location.geo %>
  </ul>

<h1>The Team</h1>
  <% @bios.each do |bio| %>
    <br><%= bio.title %>
    <br><%= bio.name %>
    <br><%= bio.info %>
    <br><%= bio.admin_user.id %>
    <br>
    <% if bio.image? %>
      <%= image_tag bio.image.profile.url %>
    <% end %>
  <% end %>

<h1>The Sponsors</h1>
  <% @sponsors.each do |sponsor| %>
    <br><%= sponsor.name %>
    <br><%= sponsor.url %>
    <br><%= sponsor.location.id %>
    <br>
    <% if sponsor.image? %>
      <%= link_to image_tag(sponsor.image.thumb.url), sponsor.url %>
    <% end %>
  <% end %>
