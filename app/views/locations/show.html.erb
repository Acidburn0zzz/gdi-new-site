<h1>Welcome to <%= @locations.location %></h1>
  <ul>
    <br><%= @locations.blurb %>
    <br><%= @locations.fb %>
    <br><%= @locations.meetup %>
    <br><%= @locations.geo %>
  </ul>

<h1>The Team</h1>
  <% @bio.each do |bio| %>
    <br><%= bio.title %>
    <br><%= bio.name %>
    <br><%= bio.info %>
    <br>
    <% if bio.image? %>
      <%= image_tag bio.image.thumb.url %>
    <% end %>
  <% end %>
